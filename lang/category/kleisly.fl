sig Compose : (type -> type) -> type
val Compose = F =>
    let open std.core in
    sig struct
        open monad.Monad F

        sig _>=>_ : {A B C:type} -> (A -> F B) -> (B -> F C) -> (A -> F C)
        val _>=>_ =
            let open std.core in
            fa fb => fa |> $ fb =<<_
    end

sig Kleisli : sig of Compose = Compose

